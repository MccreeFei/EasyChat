<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} %-5p [%t] %C{2} - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="cn.mccreefei" level="debug"/>
        <Root level="debug">
            <AppenderRef ref="STDOUT"/>
        </Root>
    </Loggers>
</Configuration>